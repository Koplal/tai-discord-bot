{
  "name": "tai-discord-bot",
  "version": "0.1.0",
  "description": "TAI Discord Bot - Claude-powered task automation for the TAI Discord server",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "register": "tsx src/commands/register.ts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
