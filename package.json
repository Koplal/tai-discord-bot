  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "discord.js": "^14.25.0",
    "@anthropic-ai/sdk": "^0.39.0",
    "ioredis": "^5.4.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "eslint": "^9.18.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0"
  }
